public class ResultActivity extends Activity {

  @Override
  public void onCreate(Bundle bundle) {
    super.onCreate(bundle);
    Bundle extras=getIntent().getExtras();
    extras.getString("str1");
    setContentView(R.layout.activity_result);
  }
}
